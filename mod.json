{
    "geode": "4.9.0",
    "gd": {
        "win": "2.2074",
        "android": "2.2074",
        "mac": "2.2074",
        "ios": "2.2074"
    },
    "id": "ninkaz.editor_sfx",
    "name": "NinKaz's Editor SFX",
    "version": "v1.0.0",
    "developer": "NinKaz",
    "description": "Alleviate the editor's deafening silence",
    "tags": ["editor", "enhancement", "offline"],
    "resources": {
        "files": ["sfx/*.wav"]
    },
    "incompatibilities": {
        "alphalaneous.editorsounds": {
            "importance": "conflicting",
            "version": "*"
        }
    },
    "settings": {
        "volume": {
            "type": "int",
            "name": "Volume",
            "default": 50,
            "min": 0,
            "max": 200
        },
        "sound-pack": {
            "type": "custom:sound-pack",
            "name": "Sound Pack",
            "description": "Use the <cy>arrow buttons</c> to swap which sound pack is in use.\nUse the <co>folder button</co> to open the sound pack in file explorer.\nUse the <cg>plus button</c> to import or create a new sound pack.\nUse the <cr>minus button</c> to delete the selected sound pack.",
            "default": ""
        }
    }
}
